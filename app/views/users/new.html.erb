<%= form_for(@user) do |f| %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :nonPartnerInstitution %><br />
    <%= f.text_field :nonPartnerInstitution %>
  </div>
  <div class="actions">
    <%= f.submit "Create Account" %>
  </div>
<% end %>


